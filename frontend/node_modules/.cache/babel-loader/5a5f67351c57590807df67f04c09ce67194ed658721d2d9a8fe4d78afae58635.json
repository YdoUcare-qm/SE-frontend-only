ea styles */\n.main-content {\n  margin-left: 250px; /* Adjusted to match the width of the sidebar */\n  padding: 20px;\n}\n\n/* Styles for navigation bar */\n.navbar {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 10px;\n  background-color: #f0f0f0;\n}\n\n.website-name {\n  font-size: 20px;\n}\n\n.logout-button {\n  background-color: #ff6f61;\n  border: none;\n  color: white;\n  padding: 10px 20px;\n  border-radius: 5px;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.logout-button:hover {\n  background-color: #e65346;\n}\n\n/* Chat interface styles */\n.chat-interface {\n  margin-top: 20px;\n  background-color: #fff;\n  border: 1px solid #ccc;\n  border-radius: 10px;\n  padding: 20px;\n}\n\n.chat-header {\n  text-align: center;\n  margin-bottom: 20px;\n}\n\n.chat-messages {\n  max-height: 200px;\n  overflow-y: auto;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px;\n  margin-bottom: 20px;\n}\n\n.message {\n  margin-bottom: 10px;\n}\n\n.message.sent {\n  text-align: right;\n}\n\n.chat-input {\n  display: flex;\n}\n\n.chat-input textarea {\n  flex: 1;\n  resize: none;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 5px 0 0 5px;\n}\n\n.chat-input button {\n  padding: 10px 20px;\n  border: 1px solid #007bff;\n  background-color: #007bff;\n  color: #fff;\n  border-radius: 0 5px 5px 0;\n  cursor: pointer;\n  transition: background-color 0.3s ease;\n}\n\n.chat-input button:hover {\n  background-color: #0056b3;\n}\n</style>\n\n"],"mappings":"AAyEA,eAAe;EACbA,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJF,IAAI,EAAE,UAAU;QAChB;QACAG,KAAK,EAAE,mBAAmB;QAAE;QAC5BC,GAAG,EAAE,kGAAkG;QACvGC,KAAK,EAAE,sBAAsB;QAC7BC,IAAI,EAAE,CAAC,MAAM,EAAE,MAAM,EAAE,MAAM,EAAE;MACjC,CAAC;;MACDC,YAAY,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,WAAW,EAAE,aAAY,CAAE;IAC7B,CAAC;EACH,CAAC;;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX;MACAC,OAAO,CAACC,GA