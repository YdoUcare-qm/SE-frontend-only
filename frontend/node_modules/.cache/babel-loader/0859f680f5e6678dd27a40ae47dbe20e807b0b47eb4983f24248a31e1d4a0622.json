utton>\r\n        <!-- Email -->\r\n        <div class=\"user-email\">\r\n          <p>Email: {{ user.email }}</p>\r\n        </div>\r\n      </div>\r\n      <!-- Navigation links -->\r\n      <div class=\"navigation-links\">\r\n        <ul>\r\n          <li><a href=\"#\">Home</a></li>\r\n          <li><a href=\"#\">Ticket Management</a></li>\r\n          <li><a href=\"#\">Thread Management</a></li>\r\n          <li><a href=\"#\">Chat With Admins</a></li>\r\n          <li><a href=\"#\">Notifications</a></li>\r\n          <li><a href=\"#\">Settings</a></li>\r\n        </ul>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Main content area -->\r\n    <div class=\"main-content\">\r\n      <!-- Main user information section -->\r\n      <div class=\"profile\">\r\n        <!-- Navigation bar -->\r\n        <div class=\"navbar\">\r\n          <div class=\"website-name\">{{ websiteName }}</div>\r\n          <button @click=\"logoutUser\" class=\"logout-button\">Logout</button>\r\n        </div>\r\n\r\n\r\n      </div>\r\n    </div>\r\n\r\n    <div>\r\n      <div class=\"notification-container\">\r\n    <div class=\"search-box\">\r\n        <input type=\"text\" placeholder=\"Search here\" class=\"search-input\" v-model=\"searchText\">\r\n        <button class=\"filter-button\">Filter</button>\r\n    </div>\r\n    <div class=\"notification-text\">Notification</div>\r\n\r\n    <!-- Header row -->\r\n    <div class=\"notification-header\">\r\n        <div class=\"header-column\">Topic</div>\r\n        <div class=\"header-column\">Tags</div>\r\n        <div class=\"header-column\">Date Created</div>\r\n        <div class=\"header-column\">Last Reply</div>\r\n        <div class=\"header-column\">Resolution Status</div>\r\n    </div>\r\n\r\n      <!-- Loop through rows -->\r\n      <div class=\"notification-row\" v-for=\"(row, rowIndex) in rows\" :key=\"rowIndex\">\r\n        <!-- Loop through columns -->\r\n        <button class=\"notification-button\" v-for=\"(column, colIndex) in